{
    "name": "@brionmario/cli",
    "description": "Brion Mario's Personal CLI",
    "version": "0.1.0",
    "author": "Brion Mario @brionmario",
    "bin": {
        "bmp": "./bin/run"
    },
    "bugs": "https://github.com/brionmario/bmp-cli/issues",
    "dependencies": {
        "@oclif/command": "^1",
        "@oclif/config": "^1",
        "@oclif/plugin-help": "^3",
        "cli-ux": "^5.6.3",
        "inquirer": "^8.1.2",
        "path": "^0.12.7",
        "tslib": "^1"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1",
        "@oclif/test": "^1",
        "@types/chai": "^4",
        "@types/fs-extra": "^9.0.12",
        "@types/inquirer": "^7.3.3",
        "@types/mocha": "^5",
        "@types/node": "^16.7.4",
        "chai": "^4",
        "eslint": "^5.13",
        "eslint-config-oclif": "^3.1",
        "eslint-config-oclif-typescript": "^0.1",
        "fs-extra": "^10.0.0",
        "globby": "^10",
        "mocha": "^5",
        "nyc": "^14",
        "ts-node": "^8",
        "typescript": "^3.3"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/brionmario/bmp-cli",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "bmp",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "brionmario/bmp-cli",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md"
    },
    "types": "lib/index.d.ts"
}
